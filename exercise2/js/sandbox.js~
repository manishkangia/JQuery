$(document).ready(function() { 
    $( "img[alt]" ).each(function () {
       console.log(this.alt);
    });

    $( '#search input:text' ).closest('form').addClass( 'new' );
    $listItem = $( "#myList .current" );
    $listItem.next().addClass( 'current' );
    $listItem.removeClass( 'current' );
 
    $( '#specials select' ).closest( 'form' ).find( 'input.input_submit' );

    $firstLi = $( "#slideshow li:first" );
    $firstLi.addClass( 'current' );
    $firstLi.siblings().addClass( 'disabled' );
});
